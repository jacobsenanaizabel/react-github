{
  "name": "react-github",
  "version": "1.0.0",
  "description": "React app about search in github",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --colors --inline --hot",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --port $PORT --host 0.0.0.0",
    "postinstall": "webpack -p",
    "test": "jest"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobsenanaizabel/react-github.git"
  },
  "author": "Ana Izabel Jacobsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacobsenanaizabel/react-github/issues"
  },
  "homepage": "https://github.com/jacobsenanaizabel/react-github#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.51",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.1.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "identity-obj-proxy": "^3.0.0",
    "imports": "^1.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^23.3.0",
    "jest-environment-enzyme": "^6.0.2",
    "jest-enzyme": "^6.0.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.9.0",
    "popper": "^1.0.1",
    "popper.js": "^1.14.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.1.0-test.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "sinon": "git+https://github.com/simple10/sinon-webpack",
    "webfontloader": "^1.6.28",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "nock": "^9.4.1"
  }
}
